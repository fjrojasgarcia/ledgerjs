{
  "private": true,
  "name": "@ledgerhq/example-node",
  "version": "4.14.0",
  "main": "index.js",
  "license": "Apache-2.0",
  "dependencies": {
    "@ledgerhq/hw-app-btc": "^4.13.0",
    "@ledgerhq/hw-app-eth": "^4.14.0",
    "@ledgerhq/hw-transport-node-hid": "^4.13.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-register": "^6.26.0",
    "flow-bin": "^0.68.0",
    "flow-typed": "^2.4.0"
  },
  "scripts": {
    "start": "node lib/index.js",
    "flow": "flow",
    "clean": "rm -rf lib/",
    "build_old": "cd ../.. && export PATH=$(yarn bin):$PATH && cd - && babel --source-maps -d lib src",
    "build": "babel --source-maps -d lib src",
    "watch_old": "cd ../.. && export PATH=$(yarn bin):$PATH && cd - && babel --watch --source-maps -d lib src & flow-copy-source -w -v src lib",
    "watch": "babel --watch --source-maps -d lib src"
  }
}
